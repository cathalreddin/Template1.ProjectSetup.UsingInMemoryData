@model IEnumerable<Player>

<table class="table table-striped">
	<thead>
		<tr>
			<th>ID</th>
			<th>Name</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Id</td>
				<td>@item.Name</td>
			</tr>
		}
	</tbody>
</table>
<a asp-controller="Players" asp-action="Create" class="btn btn-info">
	<i class="fa fa-arrow-left"></i> Create Player
</a>